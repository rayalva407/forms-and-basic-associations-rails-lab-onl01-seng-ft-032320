<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br><br>

  <input type="text",  name="song[note_contents][]", id="song_notes_attributes_0_content">

  <input type="text",  name="song[note_contents][]", id="song_notes_attributes_1_content">


  <br><br>
  <%= f.submit %>  
<% end %>


